<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PRSpinnerView">
        <attr name="list_of_elements" format="string" />
    </declare-styleable>

    <declare-styleable name="PRToolbar">
        <attr name="toolbar_title" format="string" />
        <attr name="toolbar_type" format="enum">
            <enum name="LOGO" value="0" />
            <enum name="TITLE" value="1" />
            <enum name="TITLE_BACK" value="2" />
            <enum name="LOGO_ONLY" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="PREmptyState">
        <attr name="empty_state_type" format="enum">
        <enum name="with_buttom" value="0" />
        <enum name="witout_buttom" value="1" />
        </attr>
        <attr name="emptystate_message" format="string" />
        <attr name="emptystate_icon" format="reference|integer" />
        <attr name="emptystate_link" format="string" />
        <attr name="emptystate_errorHeader" format="string" />

    </declare-styleable>
</resources>